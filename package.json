{
  "name": "dvdstore",
  "version": "1.0.0",
  "description": "Backend service for DVD Rental Demo App",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest --config=jest.config.js",
    "deploy:dev": "sls deploy --stage dev",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts --ext .js",
    "prepare": "husky install",
    "compile": "tsc",
    "start": "sls offline start --stage dev"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdnajimahmed/dvdstore.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mdnajimahmed/dvdstore/issues"
  },
  "homepage": "https://github.com/mdnajimahmed/dvdstore#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/axios": "^0.14.0",
    "@types/jest": "^28.1.7",
    "aws-lambda": "^1.0.7",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.3",
    "commitlint": "^17.0.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serverless-offline": "^9.1.0",
    "serverless-plugin-resource-tagging": "^1.2.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-webpack": "^5.8.0",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "chai": "^4.3.6",
    "express": "^4.18.1",
    "express-http-context": "^1.2.4",
    "mysql": "^2.18.1",
    "pg": "^8.8.0",
    "typeorm": "^0.3.7",
    "winston": "^3.8.1"
  }
}
